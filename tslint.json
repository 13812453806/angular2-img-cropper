{
  "extends": "tslint:latest",
  "rules": {
    "no-constructor-vars": true,
    "ordered-imports": false,
    "member-ordering": false,
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "variable-name": [
      true,
      "check-format",
      "allow-leading-underscore",
      "ban-keywords",
      "allow-pascal-case"
    ]
  },
  "rulesDirectory": [
  ]
}