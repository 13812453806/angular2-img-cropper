{
  "name": "ng2-img-cropper",
  "version": "0.6.2",
  "description": "Angular2 Image Cropper Component",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cstefanache/angular2-img-cropper.git"
  },
  "scripts": {
    "prepublish": "tsc",
    "postinstall": "typings install",
    "typings": "typings",
